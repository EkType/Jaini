feature akhn { # Akhanda
	sub Ka.dv Halant.dv Ssa.dv by KaSsa.dv;
	sub Ja.dv Halant.dv Nya.dv by JaNya.dv;
	sub vowelIi.dv Bindu.dv by vowelIiBindu.dv;
	sub vowelIi.dv Chandrabindu.dv by vowelIiChandrabindu.dv;
	sub Pa.dv matraLru.dv by PamatraLru.dv;
	sub Pa.dv matraLruu.dv by PamatraLruu.dv;
	sub Ka.dv matraLru.dv by KamatraLru.dv;
	sub Ka.dv matraLruu.dv by KamatraLruu.dv;
#	sub Ra.dv Halant.dv ZWJ by RaNuktaHalant.dv;
} akhn;

