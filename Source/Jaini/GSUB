languagesystem DFLT dflt;
languagesystem latn dflt;
languagesystem dev2 dflt;
languagesystem dev2 MAR;
languagesystem dev2 HIN;
languagesystem deva dflt;
languagesystem deva MAR;
languagesystem deva HIN;



#--------------------------------------------------------------------


feature locl { # Localized forms
	script dev2;
	language MAR;
		sub Sha.dv by Sha.MAR.dv;
		sub La.dv by La.MAR.dv;

	script deva;
	language MAR;
		sub Sha.dv by Sha.MAR.dv;
		sub La.dv by La.MAR.dv;
} locl;


#--------------------------------------------------------------------

feature nukt { # Nukta consonants

lookup nukt1 {
	sub Ka.dv Nukta.dv by KaNukta.dv;
	sub Kha.dv Nukta.dv by KhaNukta.dv;
	sub Ga.dv Nukta.dv by GaNukta.dv;
	sub Ja.dv Nukta.dv by JaNukta.dv;
	sub Dda.dv Nukta.dv by DdaNukta.dv;
	sub Ddha.dv Nukta.dv by DdhaNukta.dv;
	sub Pha.dv Nukta.dv by PhaNukta.dv;
	sub Ya.dv Nukta.dv by YaNukta.dv;
	sub Na.dv Nukta.dv by NaNukta.dv;
	sub Ra.dv Nukta.dv by RaNukta.dv;
	sub Lla.dv Nukta.dv by LlaNukta.dv;
} nukt1;
} nukt;


